{
  "name": "mu-css",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "sass": "npm run sass:dev && npm run sass:prod",
    "sass:dev": "sass scss:public/css/uncompressed --style expanded",
    "sass:prod": "sass scss:public/css/minified --style compressed",
    "build": "npm run sass",
    "watch:dev": "sass --watch scss:public/css/uncompressed --style expanded",
    "watch:prod": "sass --watch scss:public/css/minified --style compressed",
    "watch": "npm run watch:dev && npm run watch:prod"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "sass": "^1.81.0"
  }
}
